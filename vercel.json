{
"version": 2,
"builds": [
  {
    "src": "app.js",
    "use": "@vercel/node"
  }
],
"routes": [
  {
    "src": "/(.*)",
    "dest": "app.js",
    "methods": [
      "GET",
      "POST",
      "PUT",
      "PATCH",
      "OPTIONS",
      "DELETE"
    ]
  },
  {
    "src": "/static/(.*)",
    "dest": "/static/$1"
}
],
"outputDirectory": "staticfiles_build"
}
